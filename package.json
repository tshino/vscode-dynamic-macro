{
    "name": "dynamic-macro",
    "displayName": "Dynamic Macro",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Keymaps",
        "Other"
    ],
    "keywords": [
        "dynamic macro",
        "keyboard macro",
        "macro"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/tshino/vscode-dynamic-macro.git"
    },
    "activationEvents": [
        "onStartupFinished",
        "onCommand:dynamic-macro.repeat"
    ],
    "main": "./src/extension.js",
    "contributes": {
        "keybindings": [
            {
                "key": "ctrl+alt+l",
                "command": "-kb-macro.wrap"
            },
            {
                "key": "ctrl+alt+l",
                "command": "dynamic-macro.repeat"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@vscode/test-electron": "^2.2.2",
        "eslint": "^8.31.0",
        "glob": "^8.0.3",
        "mocha": "^10.2.0",
        "vsce": "^2.15.0"
    },
    "extensionDependencies": [
        "tshino.kb-macro"
    ]
}
